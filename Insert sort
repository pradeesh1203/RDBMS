Insert sort 

 include<iostream.h> 
#include<conio.h> 
void main() 
{ 
int arr[100]; 
int size,i,j,temp; 
clrscr(); 
cout<<"INSERTION SORT\n"; 
cout<<"==============\n"; 
cout<<"enter the size of array:"; 
cin>>size; 
cout<<"enter the elements in array:\n"; 
for(i=0;i<size;i++) 
{ 
cin>>arr[i]; 
} 
for(i=0;i<size;i++) 
{ 
for(j=i+1;j<size;j++) 
{ 
if(arr[j]<arr[i]) 
{ 
temp=arr[i]; 
arr[i]=arr[j]; 
arr[j]=temp; 
} 
}
} 
cout<<"element sorted in ascending order:\n"; 
for(i=0;i<size;i++) 
{ 
cout<<arr[i]<<endl; 
} 
getch(); 
}
